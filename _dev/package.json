{
  "name": "prestarocket-dev-tools",
  "version": "1.0.0",
  "description": "Tools to help while developing the Classic theme",
  "main": "index.js",
  "scripts": {
    "build": "postcss ./css/tailwindcsc.scss -o ./css/theme.scss & webpack -p",
    "start": "webpack -p -w",
    "buildtw": "postcss ./css/tailwindcsc.scss -o output.css",
    "debug": "webpack --mode development --devtool source-map --watch",
    "sass": "webpack --mode development --onlysass true --devtool source-map --watch",
    "watch": "webpack --progress --colors --debug --display-chunks --watch",
    "fulldebug": "webpack --mode development --debug --devtool source-map --output-pathinfo --display-chunks --display-error-details --watch --progress --colors"
  },
  "author": "Prestarocket",
  "license": "AFL-3.0",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^9.8.6",
    "babel-loader": "^8.2.1",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "file-loader": "^5.1.0",
    "jquery": "^3.5.1",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.1.7",
    "postcss-cli": "^8.2.0",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "tailwindcss": "^1.9.6",
    "terser-webpack-plugin": "^2.3.8",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "bootstrap-touchspin": "^4.3.0",
    "lazysizes": "^5.2.2",
    "material-design-icons": "^3.0.1",
    "popper.js": "^1.16.1",
    "postcss-import": "^13.0.0"
  }
}
